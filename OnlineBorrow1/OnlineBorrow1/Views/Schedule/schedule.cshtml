@model IEnumerable<OnlineBorrow1.Models.schedule>
@{
    ViewBag.Title = "实验室课表";
}

<div class="container" style="border: 1px solid #ddd;background-color:white">

    <div class="row clearfix">
        <div class="col-md-12 column">
            <div class="form-group" style="padding-top: 10px">
                <label for="inputEmail3" class="col-sm-2 control-label" style="font-size: 20px">课表查询</label>
                <div class="col-sm-2">
                    <a href="~/Schedule/schedule?classId=121" style="font-size: 20px">计121</a>
                </div>

                <div class="col-sm-2">
                    <a href="@Url.Content("~/Schedule/schedule?classId=223")" style="font-size: 20px">计223</a>
                </div>

                <div class="col-sm-2">
                    <a href="@Url.Content("~/Schedule/schedule?classId=315")" style="font-size: 20px">计315</a>
                </div>

                <div class="col-sm-2">
                    <a href="@Url.Content("~/Schedule/schedule?classId=310")" style="font-size: 20px">计310</a>
                </div>

                <div class="col-sm-2">
                    <a href="@Url.Content("~/Schedule/schedule?classId=321")" style="font-size: 20px">计321</a>
                </div>
            </div>
        </div>
        @foreach (var item in Model)
        {
            <div class="col-md-12" style="text-align: center; padding: 20px">
                <img src ="@Url.Content(item.imgpath)" />
            </div>
        }
    </div>
</div>



